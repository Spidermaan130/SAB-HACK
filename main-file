-- Secure Key Prompt
local function promptKey()
    local Frame = create("KeyEntryFrame","Frame",{
        AnchorPoint=Vector2.new(0.5,0.5),Position=UDim2.new(0.5,0,0.5,0),
        Size=UDim2.new(0,325,0,140),BackgroundColor3=Color3.fromRGB(45,45,45),
        BorderSizePixel=0
    },ScreenGui)
    create("UICorner","UICorner",{CornerRadius=UDim.new(0.16,0)},Frame)
    local Prompt = create("Prompt","TextLabel",{
        Text="Enter Access Key",Font=Enum.Font.FredokaOne,
        TextColor3=Color3.new(1,1,1),BackgroundTransparency=1,
        Size=UDim2.new(1,0,0.29,0),Position=UDim2.new(0,0,0.05,0),TextScaled=true
    },Frame)
    local Box = create("KeyBox","TextBox",{
        Size=UDim2.new(0.85,0,0.26,0),Position=UDim2.new(0.08,0,0.36,0),
        Font=Enum.Font.GothamBold,TextColor3=Color3.fromRGB(230,30,80),
        BackgroundColor        BorderSizePixel=0,ClearTextOnFocus=false
    },Frame)
    create("UICorner","UICorner",{CornerRadius=UDim.new(0.13,0)},Box)
    local Btn = create("Submit","TextButton",{
        Size=UDim2.new(0.7,0,0.22,0),Position=UDim2.new(0=Color3.new(1,1,1),
        BackgroundColor3=Color3.fromRGB(20,100,255),TextScaled=true,
        Text="Unlock",BorderSizePixel=0
    },Frame)
    create("UICorner","UICorner",{CornerRadius=UDim.new(0.14,0)},Btn)
    local Incorrect = create("IncorrectLabel","TextLabel",{
        Text="",Font=Enum.Font.SourceSansItalic,TextColor3=Color3.fromRGB(255,50,64),
        BackgroundTransparency=1,Size=UDim2.new(1,0,0.17,0),
        Position=UDim2.new(0,0,0.92,0),TextScaled=true,Visible=false
    },Frame)
    local function tryKey()
        if Box.Text == ACCESS_KEY then
            Frame:Destroy()
        else
            Incorrect.Text = "Incorrect Key!"
            Incorrect.Visible = true
            wait(1.1)
            Incorrect.Visible = false
        end
    end
    Btn.MouseButton1Click:Connect(tryKey)
    Box.FocusLost:Connect(function(enter) if enter then tryKey() end end)
    -- Touch OK for enter key
end
promptKey()

-- Draggable Circular Button (Red/Blue)
local DragRadius = 44
local MainButton,DragRadius*2),AnchorPoint=Vector2.new(0,0),
    Position=UDim2.new(0.065,0,0.17,0),BackgroundTransparency=1,
    Image="rbxassetid://3926307971", -- circle icon
    ImageColor3=Color3.fromRGB(5,0.5),Position=UDim2.new(0.5,0,0.5,0),
    Size=UDim2.new(.72,0,.72,0),BackgroundColor3=Color3.fromRGB(65,170,255),BackgroundTransparency=0.08,ZIndex=2
},MainButton)
create("UICorner","UICorner",{CornerRadius=UDim.new(1,0)},Blue dragging,offset=false,Vector2.new()
    local UIS=game:GetService("UserInputService")
    MainButton.InputBegan:Connect(function(input)
        if input.UserInputType==Enum.UserInputType.Touch or input.UserInputType==Enum.UserInputType.MouseButton1 then
            dragging=true
            offset=Vector2.new(input.Position.X, input.Position.Y)-Vector2.new(MainButton.AbsolutePosition.X, MainButton.AbsolutePosition.Y)
        end
    end)
    UIS.InputChanged:Connect(function(input)
        if dragging and (input.UserInputType==Enum.UserInputType.Touch or input.UserInputType==Enum.UserInputType.MouseMovement) then
            local x=input.Position.X-offset.X
            local y=input.Position.Y-offset.Y
            MainButton.Position=UDim2.fromOffset(x,y)
        end
    end)
    local function dragEnd(input)
        if input.UserInputType==Enum.UserInputType.Touch or input.UserInputType==Enum.UserInputType.MouseButton1 then
            dragging=false
        end
    end
    MainButton.InputEnded:Connect(dragEnd)
end

