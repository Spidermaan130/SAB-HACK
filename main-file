-- Use this to connect to your remote or spawning logic
local function trySpawn(unit)
    if not unit then return end
customize as needed)
    local remotes = ReplicatedStorage:FindFirstChild("Remotes")
    if remotes and remotes:FindFirstChild("Spawn") then
        remotes.Spawn:FireServer(unit)
    end
end
SpawnBtn.MouseButton1Click:Connect(function() trySpawn(SelectedUnit) end)

-- Auto-loop
local IsAutoLooping = false
local AutoBtn = create("AutoBtn","TextButton",{
    Size=UDim2.new(0.91,0,0.15,0),Position=UD615,0),
    BackgroundColor3=Color3.fromRGB(30,180,80),Text="Enable Auto-Loop",
    TextColor3=Color3.new(1,1,1),Font=Enum.Font.FredokaOne,TextScaled=true,BorderSizePixel=0,ZIndex=17
},ActionFrame)
create("UICorner","UICConnect(function()
    if not SelectedUnit then return end
    IsAutoLooping = not IsAutoLooping
    AutoBtn.Text = IsAutoLooping and "Disable Auto-Loop" or "Enable Auto-Loop"
    if IsAutoLooping then
        loopThread = coroutine.create(function()
            while IsAutoLooping do
                trySpawn(SelectedUnit)
                wait(1.1)
            end
        end)
        coroutine.resume(loopThread)
    end
end)

