local SpawnerTab = Window:CreateTab("Wiki Spawner", 4483362458)
local PerfTab = Window:CreateTab("Performance", 10734891155)

-- 1. SELECTION & MUTATION (Spawner Tab)
local SelectedRarity = "OG"
local SelectedRot = "Skibidi Toilet"
local SelectedMut = "None"

SpawnerTab:CreateDropdown({
   Name = "1. Select Rarity Tier",
   Options = {"OG", "Secret", "Brainrot God", "Mythic", "Legendary", "Epic", "Rare/Common"},
   CurrentOption = {"OG"},
   Callback = function(Option) SelectedRarity = Option[1] end,
})

SpawnerTab:CreateDropdown({
   Name = "2. Select Brainrot (217+ Items)",
   Options = BrainrotDatabase["OG"],
   CurrentOption = {"Skibidi Toilet"},
   Flag = "RotDrop",
   Callback = function(Option) SelectedRot = Option[1] end,
})

SpawnerTab:CreateDropdown({
   Name = "3. Select Mutation (Wiki Traits)",
   Options = WikiMutations,
   CurrentOption = {"None"},
   Callback = function(Option) SelectedMut = Option[1] end,
})

-- 2. EXECUTION: LOOP (LEFT) & SPAWN (RIGHT)
SpawnerTab:CreateSection("Execution Control")
local Looping = false

local function PerformSpawn()
    if SelectedRot == "None" then return end
    local Remote = game:GetService("ReplicatedStorage"):FindFirstChild("SpawnEvent") or 
                 game:GetService("ReplicatedStorage"):FindFirstChildOfClass("RemoteEvent")
    if Remote then
        Remote:FireServer(SelectedRot, SelectedMut, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
    end
end

SpawnerTab:CreateToggle({
   Name = "Loop Spawning (Left)",
   CurrentValue = false,
   Flag = "LoopToggle",
   Callback = function(Value)
      Looping = Value
      task.spawn(function()
         while Looping do
            PerformSpawn()
            task.wait(0.12) -- Ultra-fast for new Delta
         end
      end)
   end,
})

SpawnerTab:CreateButton({
   Name = "ðŸš€ SPAWN SELECTED (Right)",
   Callback = function()
      PerformSpawn()
      Rayfield:Notify({Title = "Ghaloulix9 Success", Content = "Spawned: " .. SelectedRot, Duration = 2})
   end,
})

-- 3. FPS BOOSTER (Performance Tab)
PerfTab:CreateSection("Delta FPS Optimization")

PerfTab:CreateButton({
   Name = "âš¡ BOOSTER: MAX FPS (Remove Shadows/Particles)",
   Callback = function()
      -- Remove Shadows and Graphics Bloat
      game:GetService("Lighting").GlobalShadows = false
      game:GetService("Lighting").FogEnd = 9e9
      settings().Rendering.QualityLevel = 1
      
      -- Clean Workspace
      for _, v in pairs(game:GetDescendants()) do
         if v:IsA("BasePart") then
            v.Material = Enum.Material.Plastic
            v.Reflectance = 0
         elseif v:IsA("Decal") or v:IsA("Texture") then
            v:Destroy()
         elseif v:IsA("ParticleEmitter") or v:IsA("Trail") then
            v.Enabled = false
         end
      end
      
      Rayfield:Notify({
         Title = "FPS Boosted!",
         Content = "Graphics lowered for Ghaloulix9's chaos mode.",
         Duration = 3,
      })
   end,
})

PerfTab:CreateToggle({
   Name = "Disable 3D Rendering (EXTREME FPS)",
   CurrentValue = false,
   Callback = function(Value)
      game:GetService("RunService"):Set3dRenderingEnabled(not Value)
      if Value then
          Rayfield:Notify({Title = "Extreme Mode", Content = "Rendering disabled. Script still running in background!", Duration = 3})
      end
   end,
})

-- FINAL INITIALIZATION
Rayfield:Notify({
   Title = "GHALOULIX9'S BEST HACKS",
   Content = "Running on Delta v2026. Wiki database synchronized.",
   Duration = 5,
})
